<Window x:Class="mdk0202.metod"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:mdk0202"
        mc:Ignorable="d"
        Title="metod" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="tbprType" HorizontalAlignment="Left" Margin="234,26,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="324" PreviewTextInput="TextBox_PreviewTextInput"/>
        <TextBox x:Name="tbmatType" HorizontalAlignment="Left" Margin="234,75,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="324" PreviewTextInput="TextBox_PreviewTextInput_3"/>
        <TextBox x:Name="tbRequiredAmount" HorizontalAlignment="Left" Margin="234,124,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="324" PreviewTextInput="TextBox_PreviewTextInput_4"/>
        <TextBox x:Name="tbWidth" HorizontalAlignment="Center" Margin="0,222,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="324" PreviewTextInput="TextBox_PreviewTextInput_1"/>
        <TextBox x:Name="tbHeight" HorizontalAlignment="Center" Margin="0,276,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="324" PreviewTextInput="TextBox_PreviewTextInput_2"/>
        <Label Content="Тип продукта(код)" HorizontalAlignment="Left" Margin="234,0,0,0" VerticalAlignment="Top"/>
        <Label Content="Тип материала(код)" HorizontalAlignment="Left" Margin="234,49,0,0" VerticalAlignment="Top"/>
        <Label Content="Требуемое количество продукта" HorizontalAlignment="Left" Margin="234,98,0,0" VerticalAlignment="Top"/>
        <Label Content="Ширина" HorizontalAlignment="Left" Margin="238,196,0,0" VerticalAlignment="Top"/>
        <Label Content="Высота" HorizontalAlignment="Left" Margin="238,245,0,0" VerticalAlignment="Top"/>
        <Button x:Name="raschetbt" Content="Посчитать" Background="#FF0C4882" HorizontalAlignment="Center" Margin="0,304,0,0" VerticalAlignment="Top" Height="30" Width="114" Foreground="White" Click="raschetbt_Click_1" />
        <Label x:Name="labelOutput" Content="Label" HorizontalAlignment="Left" Margin="295,365,0,0" VerticalAlignment="Top" FontSize="16" FontWeight="Bold" Width="206" HorizontalContentAlignment="Center"/>
        <TextBox x:Name="tbWarehouseAmount" HorizontalAlignment="Left" Margin="234,173,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="324" PreviewTextInput="TextBox_PreviewTextInput_4"/>
        <Label Content="Количество продукта на складе" HorizontalAlignment="Left" Margin="234,147,0,0" VerticalAlignment="Top"/>
        <Label Content="Требуемое количество материала:" HorizontalAlignment="Left" Margin="295,339,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
